        // Cancel Order
        function RejectOrderScript(value) {
            var OrderID = value.title;
            var ReasonMsg = $("#ctl00_txtMstReasonCode").val().trim();
            if (ReasonMsg.length > 0) {
                $(".fade-layer").hide();
                $("#ctl00_pnlMstCancelOrder").hide();
                ShowLoader(true); ;
                CheckSessionExistance(siteurl);
                $.ajax({
                    type: "POST",
                    url: siteurl + "UserPages/WSUser.asmx/RejectOrder",
                    data: '{"OrderID": "' + OrderID + '","ReasonMsg": "' + ReasonMsg + '"}',
                    contentType: "application/json; charset=utf-8",
                    processData: false,
                    dataType: "json",
                    success: function(msg) {
                        BindNotificationItems();
                        ShowLoader(false);
                        //GeneralAlertMsg(msg.d);
                        GeneralAlertMsg(msg.d);
                    },

                    error: function(x, e) {
                        if (x.status == 500) {
                            GeneralAlertMsg("An error has occurred during processing your request.");
                        }
                    }
                });
            }
            else {
                GeneralAlertMsg('Please enter reason for cancel');
            }
        }
        function ShowReasonForCancelPopup(OrderID) {
            $("#CancelOrder-master-popup").css('top', '0');
            $(".fade-layer").show();
            $("#ctl00_pnlMstCancelOrder").show();
            // set id to button tittle
            var rejectbtn = document.getElementById('lnkMstCancelOrderSubmit');
            rejectbtn.title = OrderID;
            $("#ctl00_txtMstReasonCode").val('');
            //.setAttribute('onclick', 'RejectOrderScript(' + OrderID + ',this);');
        }
        // Approve/Reject User
        function ApprovalUserScript(regisID, flagtype) {
            CheckSessionExistance(siteurl);
            ShowLoader(true);
            var opurl = new String();
            if (flagtype == "true") {
                opurl = siteurl + "UserPages/WSUser.asmx/ApproveUser";
            }
            else {
                opurl = siteurl + "UserPages/WSUser.asmx/RejectUser";
            }
            $.ajax({
                type: "POST",
                url: opurl,
                data: '{"RegistrationID": "' + regisID + '"}',
                contentType: "application/json; charset=utf-8",
                processData: false,
                dataType: "json",
                success: function(msg) {
                    BindNotificationItems();
                    ShowLoader(false);
                    //GeneralAlertMsg(msg.d);
                    GeneralAlertMsg(msg.d);
                },

                error: function(x, e) {
                    if (x.status == 500) {
                        GeneralAlertMsg("An error has occurred during processing your request.");
                    }
                }
            });
        }
