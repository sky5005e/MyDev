            $(".containerNavigation .wrapper").find('li').each(function() {
                $(this).on('click', function(e) {
                    if (ActiveId == "")
                        ActiveId = $(".containerNavigation .wrapper").find('li.active').attr("id");

                    $(".containerNavigation .wrapper").find('li').removeClass('active');
                    $(this).addClass('active');
