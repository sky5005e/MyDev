        $(document).ready(function() {
            //common code for all pages...        
        
            $('input').iCheck({
                checkboxClass: 'icheckbox_flat',
                radioClass: 'iradio_flat'
            });
            
            $('.apple_checkbox input, .icheckbox_flat input').on('ifClicked', function() {
                var checkBox = $(this);                
                $(checkBox).click();
                
                var moasOption = $(checkBox).parents(".chkMOAS").length > 0;
                var approverOption = $(checkBox).parents(".approver").length > 0;
                
                if (moasOption || approverOption) {
                    $(checkBox).attr('checked', !$(checkBox).is(":checked"));
                    if (moasOption)                        
                        $(".pnlMOASApprovers").toggle($(".chkMOAS input:checked").length > 0);
                    else
                        $.updatePriority($(checkBox));
                }
            });
            
            $(".current-tab input, .current-tab select, #container input, #container select, #container textarea").on('input, change', function() {
                TabInfoChanged();
            });
            
            $(".current-tab input[type='checkbox'], #container input[type='checkbox']").one('click', function(e) {
                TabInfoChanged();
            });
            
            $(".setDatePicker").datepicker({
                changeMonth: true,
                changeYear: true
            });            
            
            $(".first-field").on("keypress blur", function(e) {
                $.properCase(this);                
            });
            
            $(".containerNavigation ul li#" + $("#hdnActiveLink").val() + "").addClass('active');

//            $(window).scroll(function() {
//                $("#ctl00_hdnScrollY").val($(window).scrollTop());
//            });

//            $(window).scrollTop($("#ctl00_hdnScrollY").val());
            
            //common code for all pages ends here...

            // Display this RSS Feed on page Load
            getRSSFeedFirsTime();

            function tick2() {
                $('#movingUL li:first').slideUp(function() { $(this).appendTo($('#movingUL')).slideDown(); });
            }
            setInterval(function() { tick2() }, 7000);
            // For Already Added item
