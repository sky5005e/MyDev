        var ActiveId = "";
        function DisplayContactUs() {

            $('.contact-link').toggleClass('opened');
            if ($('.contact-link').hasClass('opened')) {
                $('.welcome-txt').slideUp({ queue: false });
                $('.contact-block').slideDown({ queue: false });
                $('body').removeClass('userLogin');
                $('body').addClass('afterLogin contact-page');
            }
            else {
                $('.welcome-txt').slideDown(function() {
                    $('body').addClass('userLogin');
                    $('body').removeClass('afterLogin contact-page');
                    $('.contact-link').removeClass('active');
                    //SetActive
                    $("#" + ActiveId).addClass('active');
                    ActiveId = "";
                });
                $('.contact-block').slideUp({ queue: false });


            }

        }
        
        
           function CloseMediaHelpVideo(iframeid) {
           
