        // Save Notes Info
        function SaveNotes(value) {
            var ServiceTicketID = value.title;
            var NoteMsg = $("#ctl00_txtReplyMsg").val().trim();
            if (NoteMsg.length > 0) {
                $("#ctl00_pnlMessage").hide();
                ShowLoader(true);
                CheckSessionExistance(siteurl);
                $.ajax({
                    type: "POST",
                    url: siteurl + "UserPages/WSUser.asmx/SaveMessage",
                    data: '{"ServiceTicketID": "' + ServiceTicketID + '","NoteMsg": "' + NoteMsg + '"}',
                    contentType: "application/json; charset=utf-8",
                    processData: false,
                    dataType: "json",
                    success: function(msg) {
                        BindNotificationItems();
                        $(".fade-layer").show();
                        ShowLoader(false);
                        //GeneralAlertMsg(msg.d);
                        GeneralAlertMsg(msg.d);
                    },

                    error: function(x, e) {
                        if (x.status == 500) {
                            GeneralAlertMsg("An error has occurred during processing your request.");
                        }
                    }
                });
            }
            else {
                GeneralAlertMsg('Please enter Notes');
            }

        }
