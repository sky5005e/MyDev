    <script type="text/javascript">
    function GetHelpVideo(ModuleName,PageOrPupupName) {
    
    $.ajax({
        type: "POST",
        url: siteurl + "UserPages/WSUser.asmx/GetHelpVideoOrDoc",
        data: "{'Type':'help video', 'ModuleName':'" + ModuleName + "','PageOrPopupTitle':'" + PageOrPupupName + "'}",
        contentType: "application/json; charset=utf-8",
        dataType: "json",
        success: function(msg) {
                        if(msg.d!="")
                        {
                        ShowHelpVideo(msg.d);
                        }
        },
        error: function(x, e) {
        }
     });
    }
    
    
    function GetDocumentLink(ModuleName,PageOrPupupName) {
    
    $.ajax({
        type: "POST",
        url: siteurl + "UserPages/WSUser.asmx/GetHelpVideoOrDoc",
        data: "{'Type':'document link', 'ModuleName':'" + ModuleName + "','PageOrPopupTitle':'" + PageOrPupupName + "'}",
        contentType: "application/json; charset=utf-8",
        dataType: "json",
        success: function(msg) {
                        if(msg.d!="")
                        {
                        window.open(siteurl + 'DocmentStorageCenter/' + msg.d );
                        }
        },
        error: function(x, e) {
        }
     });
    }
    function ShowHelpVideo(VidUrl) {
         
            $("#help-video-block").css('top', '0');
            $(".fade-layer").show();
            $("#help-video-block").show();
            var videoURL =VidUrl;
            $("#HelpVideoIframe").attr("src", videoURL);
            SetPopUpAtTop();
        }



  function CloseHelpVideo() {
           
            $("#help-video-block").hide();
            $("#HelpVideoIframe").attr("src", '');
        }
    </script>

