        // Get RSS Feed for Firstime
        function getRSSFeedFirsTime() {
            CheckSessionExistance(siteurl);
            $.ajax({
                type: "POST",
                url: siteurl + "UserPages/WSUser.asmx/GetRSSFeedsOnPageLoad",
                data: '{}',
                contentType: "application/json; charset=utf-8",
                processData: false,
                dataType: "json",
                success: function(msg) {
                    var objlst = msg.d;
                    if (objlst != undefined) {
                        $("#movingdiv").html(objlst.HItemsHtml);
                    }
                },
                error: function(x, e) {
                    if (x.status == 500) {
                        GeneralAlertMsg("An error has occurred during processing your request.");
                    }
                }
            });
        }


        // For Bind Cart Items
        function BindNotificationItems() {            
            CheckSessionExistance(siteurl);
            var dvNotifications = $("#dvNotifications");
            var lblTotalNotification = $("#ctl00_lblTotalNotification");
            $.ajax({
                //beforeSend: function() { ShowLoader(true); }, //Show spinner
                //complete: function() { ShowLoader(false); }, //Hide spinner
                type: "POST",
                url: siteurl + "UserPages/WSUser.asmx/GetNotificationDetails",
                data: '{}',
                contentType: "application/json; charset=utf-8",
                processData: false,
                dataType: "json",
                success: function(msg) {
                    var objlst = msg.d;
                    if (objlst != undefined) {
                        dvNotifications.html(objlst.HItemsHtml);
                        lblTotalNotification.html(objlst.TotalNumber);
                        // For Sub Items  of Notifications scroll
                         var ua = navigator.userAgent;
                         var CurrEvent = '';
                         
                         if (ua.match('/iPhone/i') || ua.match('iPhone;') || ua.match('/iPod/i') || ua.match('iPod;') || ua.match('/iPad/i') || ua.match('iPad;'))
                                    CurrEvent = "touchstart";
                         else
                                    CurrEvent = "click";
                            
                        
                        $('.notification-header').bind(CurrEvent,function(e) {
                            $(this).parent().find('.notifyHeader1').slideToggle();
                        });
                        
                        $(".notifyHeader1").niceScroll(
			            {
			                touchbehavior: false,
			                cursorcolor: "#666",
			                cursoropacitymax: 0.7,
			                cursorwidth: 6,
			                cursorborder: "1px solid #666",
			                cursorborderradius: "8px",
			                autohidemode: "scroll"
			            });
			            $(".innernNavigation li:last-child").addClass("last");
                        $(".innernNavigation > li .notificationSubnav").hide();
                        $(".innernNavigation > li").hover(function() {
                            $(this).find('.notificationSubnav').show();
                        }, function() {
                            $('.notificationSubnav').hide();
                        });
                        $('.innernNavigation > li > a .num-display span').css('display','inline-block');
                        //GeneralAlertMsg($(".header-cart-btn a").length);
                        //SetLinkforIpadForAnchorTag($('.innernNavigation li #dvNotifications a,.header-cart-btn a'),CurrEvent);
                    }
                },
                error: function(x, e) {
                    if (x.status == 500) {
                        GeneralAlertMsg("An error has occurred during processing your request.");
                    }
                }
            });
        }
        // For Bind Cart Items
        function BindCartItems() {
            CheckSessionExistance(siteurl);
            var dvCart = $("#dvCart");
            var lblCartAmount = $("#ctl00_lblCartTotalAmount");
            $.ajax({
                type: "POST",
                url: siteurl + "UserPages/WSUser.asmx/GetAllCartItems",
                data: '{}',
                contentType: "application/json; charset=utf-8",
                processData: false,
                dataType: "json",
                success: function(msg) {
                    var objlst = msg.d;
                    if (objlst != undefined) {
                        dvCart.html(objlst.HItemsHtml);
                        lblCartAmount.html(objlst.TotalNumber);
                    }
                },

                error: function(x, e) {
                    if (x.status == 500) {
                        GeneralAlertMsg("An error has occurred during processing your request.");
                    }
                }
            });
        }

        // For Remove Items from  Cart
        function RemoveCartItemsFromMaster() {
            CheckSessionExistance(siteurl);
            // As per Ken: display below alert
            GeneralAlertMsg("You are about to delete all of the items in your cart.");
            var _myShoppingCartID = new String(); // to pass _myShoppingCartID field. here we will pass null value
            $.ajax({
                type: "POST",
                url: siteurl + "UserPages/WSUser.asmx/RemoveItemFromCart",
                data: '{"MyShoppingCartID": "' + _myShoppingCartID + '"}',
                contentType: "application/json; charset=utf-8",
                processData: false,
                dataType: "json",
                success: function(msg) {
                    GeneralAlertMsg("Cart Items successfully removed.");
                    BindCartItems();
                    $("div.cart-remove").each(function() {
                        if ($(this).css("display") == "block") {
                            $(this).siblings("div.cart-caption").show();
                            $(this).hide();
                        }
                        else {
                        }
                    });
                },

                error: function(x, e) {
                    if (x.status == 500) {
                        GeneralAlertMsg("An error has occurred during processing your request.");
                    }
                }
            });
        }

        // Approve Order
        function ApprovalOrderScript(OrderID) {
            ShowLoader(true);
            CheckSessionExistance(siteurl);
            $.ajax({
                type: "POST",
                url: siteurl + "UserPages/WSUser.asmx/ApproveOrder",
                data: '{"OrderID": "' + OrderID + '"}',
                contentType: "application/json; charset=utf-8",
                processData: false,
                dataType: "json",
                success: function(msg) {
                    BindNotificationItems();
                    ShowLoader(false);
                    //GeneralAlertMsg(msg.d);
                    GeneralAlertMsg(msg.d);
                },

                error: function(x, e) {
                    if (x.status == 500) {
                        GeneralAlertMsg("An error has occurred during processing your request.");
                    }
                }
            });
